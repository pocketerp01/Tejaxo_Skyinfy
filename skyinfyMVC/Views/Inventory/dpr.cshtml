<script type="text/javascript">

    function calcissue() {
        try {

            var heads = $("[id*=col_]");
            var rows = $(heads).eq(0).find("[id*=row_]");
            for (var r = 0; r < rows.length; r++) {
                var row = $("[id=row_" + r + "]");
                //var qtystk = parseFloat(row[5].innerText);
                var qtychl = Math.abs(parseFloat(row[6].innerText));
                var qtyout = Math.abs(parseFloat(row[7].innerText.trim()));
                if (qtyout > qtychl) {
                    if (!shown) {
                        alert('Issue Qty can not be more than Chl Qty');
                        row[7].innerText = qtychl;
                        shown = true;
                    }
                    else {
                        shown = false;
                    }
                    return;
                }
                var qtybal = qtychl - qtyout;
                row[8].innerText = qtybal;
                row[7].innerText = qtyout;
            }
        }
        catch (err) { alert(err); return; }
    }

    function calcpro() {
        try {
            var heads = $("[id*=col_]");
            var rows = $(heads).eq(0).find("[id*=row_]");
            for (var r = 0; r < rows.length; r++) {
                var row = $("[id=row_" + r + "]");

                var qtypro = Math.abs(parseFloat(row[6].innerText));
