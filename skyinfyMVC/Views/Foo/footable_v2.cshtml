            $(document).ready(function () {

                var checktype = '@sgen.GetSession(MyGuid, "CHECKTYPE").ToString()';
                if (checktype != 2) {
                    $("[type=checkbox][id=cbSelectAll]").hide();
                }
                else $("[type=checkbox][id=cbSelectAll]").show();

                //    $(this).find(".checkbox").prop('checked', true);
