<script type="text/javascript">


    function callme(evt) {
        //evt.preventDefault();



        var mands = $(".sa-mandatory");
        var pg = 1;
        var item;
        var allok = true;
        for (var m = 0; m < mands.length; m++) {
            var ok = mands[m].checkValidity();
            var id = mands[m].id;
            item = mands[m];


            if (id == "DropDownList1") {
                pg = 2;
            }
            else if (id == "DropDownList2") {
                pg = 3;
            }
            else if (id == "DropDownList3") {
                pg = 4;
            }
            else if (id == "txt_withdrawl") {
                pg = 6;
            }
            if (!ok) {
                allok = false;
                break;
            }

        }

        if (!allok) {
            var navListItems = $('div.setup-panel div a');
            navListItems.removeClass('btn-primary').addClass('btn-default');
            $('div.setup-panel div').find("#a" + pg + "").addClass('btn-primary');
            $('div.setup-panel div').find("#a" + pg + "")[0].click();
            var allWells = $('.setup-content');
            allWells.hide();
            $("[id=step-" + pg + "]").show();
            item.reportValidity();
            return false;
        }
        else return true;


    }

