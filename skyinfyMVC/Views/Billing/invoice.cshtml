    function show_date1() {
        var stdt = $("#txt_mstdate")[0].value;
        var heads = $("[id*=col_]");
        var rows = $(heads).eq(0).find("[id*=row_]");
        for (var r = 0; r < rows.length; r++) {

            var row = $("[id=row_" + r + "]");
            row[17].children[0].children[0].children[0].value = stdt
            //row[8].innerText = stdt;
        }
    }

    function caldiscamt() {
        try {

            var iamt = 0, sumamt = 0;

            var heads = $("[id*=col_]");
            var rows = $(heads).eq(0).find("[id*=row_]");
            var ivtype = $("#hf_sotype")[0].value;

            for (var r = 0; r < rows.length; r++) {
                var row = $("[id=row_" + r + "]");
                if (ivtype == "53" || ivtype == "54" || ivtype == "") {

                    if (row[13].innerText.trim() != "") { iamt = parseFloat(row[13].innerText.trim()).toFixed(2); }
                    var idiscrate = row[12].innerText.split('%')[0];
                    var discamt = iamt * idiscrate / 100;
                    row[15].innerText = (discamt).toFixed(2);
                }
                else {
                    if (row[14].innerText.trim() != "") { iamt = parseFloat(row[14].innerText.trim()).toFixed(2); }
                    var idiscrate = row[13].innerText.split('%')[0];
                    var discamt = iamt * idiscrate / 100;
                    row[16].innerText = (discamt).toFixed(2);
                }
                calcamt();
            }
        }
        catch (err) { alert(err); return; }
    }
    function caldiscrate() {
        try {

            var iamt = 0, discamt = 0, restamt = 0;
            var idiscrate = "";
            var heads = $("[id*=col_]");
            var rows = $(heads).eq(0).find("[id*=row_]");
