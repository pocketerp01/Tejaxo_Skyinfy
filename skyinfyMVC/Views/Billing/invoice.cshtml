            $("#div_temp").show();
            $("#div_upd").show();
            $("#div_btnupd").show();
        }
        else {
            $("#div_temp").hide();
            $("#div_upd").hide();
            $("#div_btnupd").hide();
        }
    }
    function invtemp(evt) {
          evt.preventDefault();
          var $form = $("#invtemp");

          $.ajax({
              type: "POST",
              url: "/Billing/SetSession",
              data: $form.serialize() + "&Name=model",
              dataType: "json",
              traditional: true,
              success: function (response) {
                  window.location.href = "@Url.RouteUrl(new { Controller = "Billing", Action = "invtemp" })";
              },
              error: function (err) {
                   window.location.href = "@Url.RouteUrl(new { Controller = "Billing", Action = "invtemp"})";
              }
          });
    }
    function chkcloseqty() {
        //;
        var heads = $("[id*=col_]");
        var rows = $(heads).eq(0).find("[id*=row_]");
        var balqty = "", closeqty = "";
        for (var r = 0; r < rows.length; r++) {
            var row = $("[id=row_" + r + "]");
            balqty = parseFloat(row[9].innerText.trim());
            closeqty = row[10].innerText.trim();
            if (balqty == 0) {
                closeqty = "0";
                row[10].innerText = closeqty;
            }
            if (closeqty > balqty) {
                closeqty = balqty;
                row[10].innerText = closeqty;
                showmsgJS(1, "Invoice qty should not be more than bal qty,Please Check !", 2); return;
            }
        }
    }
       function contper(checkbox) {
        var x = document.getElementById("txt_pernmdiv");
        var check = $(checkbox)[0].checked;
        if (check) {
            x.style.display = "block";
        }
        else {
            x.style.display = "none";
        }
    }
    function getchargeamt() {
        //  debugger
        var chrgamt = 0, rt = 0;
        var basic = parseFloat($("#txt_bamt")[0].value);
        var ltxtamt = $("[id=txt_amt]");
        var rate = $("[id=txt_per]");
        for (var k = 0; k < rate.length; k++) {
            rt = parseFloat(rate[k].value);//chrgamt
            chrgamt = basic * rt / 100;
            ltxtamt[k].value = chrgamt.toFixed(2);//amount
        }
    }
    function getchargerate() {
        //  debugger
        var chrgamt = 0, tamt = 0;
        var basic = parseFloat($("#txt_bamt")[0].value);
        var ltxtamt = $("[id=txt_amt]");
        var rate = $("[id=txt_per]");
        for (var k = 0; k < ltxtamt.length; k++) {
            tamt = parseFloat(ltxtamt[k].value);//chrgamt
            chrgamt = tamt / basic * 100;
            rate[k].value = chrgamt.toFixed(2);//rate
        }
    }
