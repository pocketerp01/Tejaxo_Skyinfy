@using System.Data

@{
    ViewBag.Title = "tax_invoice";
    Layout = "~/Views/Shared/_MasterPage.cshtml";
    
}

<div class="row">
    <div class="col-md-4">
        <div class="row">
            <div class="col-sm-12">
                <div class="form-group">
                    <div class='input-group'>
                        <div class="title_left">
                            <h3>
                                Add New Tax Invoice
                                @*<asp:Label runat="server" ID="lbl_heading"></asp:Label>*@
                            </h3>
                        </div>
                        <span class="input-group-addon formclass"></span>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-8">
        <div class="row">
            <div class="col-sm-2">
                <div class="form-group">
                    <div class='input-group'>
                        <button type="submit" class="btn btn-primary form-control sa-button" id="btn_new" accesskey="N"><u>N</u>ew</button>
                        <span class="input-group-addon formclass"></span>
                    </div>
                </div>
            </div>

            <div class='col-sm-2'>
                <div class="form-group">
                    <div class='input-group'>
                        <button type="submit" class="btn btn-success form-control sa-button" id="btn_save" disabled="disabled" accesskey="S"><u>S</u>ave</button>
                        <span class="input-group-addon formclass"></span>
                    </div>
                </div>
            </div>

            <div class='col-sm-2'>
                <div class="form-group">
                    <div class='input-group'>
                        <button type="submit" class="btn btn-warning form-control sa-button" causesvalidation="false" id="btn_edit" accesskey="i"><u>E</u>dit</button>
                        <span class="input-group-addon formclass"></span>
                    </div>
                </div>
            </div>

            <div class='col-sm-2'>
                <div class="form-group">
                    <div class='input-group'>
                        <button type="submit" class="btn btn-info form-control" causesvalidation="false" style="border-radius: 10px!important;" id="btn_detail" accesskey="V"><u>V</u>iew List</button>
                        <span class="input-group-addon formclass"></span>
                    </div>
                </div>
            </div>

            <div class='col-sm-2'>
                <div class="form-group">
                    <div class='input-group'>
                        <button type="submit" class="form-control sa-button sa-btncancel" causesvalidation="false" id="btn_cancel" accesskey="C"><u>C</u>ancel</button>
                        <span class="input-group-addon formclass"></span>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">

                <div class="clearfix"></div>
                <div class="x_content">

                    <div class="row">
                        <div class='col-sm-4'>
                            <label for="first-name">Client Name <span class="ClientProfileLabelColor">*</span></label>
                            <div class="form-group">
                                <div class='input-group'>
                                    <select class="form-control select2 col-md-12 col-sm-12 col-xs-12 sa-select2-single" id="ddl_client" clientidmode="Static" multiple="true" style="width: 100%; border-radius: 0px;"></select>
                                    <span class="input-group-addon formclass"></span>
                                </div>
                            </div>
                        </div>

                        <div class='col-sm-4'>
                            <label for="first-name">Invoice No. <span class="ClientProfileLabelColor">*</span></label>
                            <div class="form-group">
                                <div class='input-group'>
                                    <div class="row">
                                        <div class="col-md-6 col-sm-6">
                                            <input type="text" name="invoice" class="form-control col-md-12 col-sm-12 col-xs-12 sa-textbox" id="txt_invoic_code" />
                                        </div>

                                        <div class="col-md-6 col-sm-6">
                                            <input type="text" name="invoice" id="txt_invoice_no" class="form-control col-md-12 col-sm-12 col-xs-12 sa-textbox" />
                                        </div>
                                    </div>
                                    <span class="input-group-addon formclass"></span>
                                </div>
                            </div>

                        </div>
                        <div class='col-sm-4'>
                            <label for="first-name">Invoice Date <span class="ClientProfileLabelColor">*</span></label>
                            <div class="form-group">
                                <div class='input-group'>
                                    <input type="text" name="invoicedate" id="txt_invoice_date" class="form-control col-md-12 col-sm-12 col-xs-12 sa-textbox sa-date" />
                                    <span class="input-group-addon formclass"></span>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class='col-sm-4'>
                            <label for="first-name">Payment Terms <span class="ClientProfileLabelColor">*</span></label>
                            <div class="form-group">
                                <div class='input-group'>
                                    <select class="form-control select2 col-md-12 col-sm-12 col-xs-12 sa-select2-single" id="ddl_payment_terms" clientidmode="Static" multiple="true" style="width: 100%; border-radius: 0px;"></select>
                                    <span class="input-group-addon formclass"></span>
                                </div>
                            </div>
                        </div>
                        <div class='col-sm-4'>
                            <label for="first-name">PO Number <span class="ClientProfileLabelColor">*</span></label>
                            <div class="form-group">
                                <div class='input-group'>
                                    <input type="text" name="ponumber" id="txt_ponumber" class="form-control col-md-12 col-sm-12 col-xs-12 sa-textbox" />
                                    <span class="input-group-addon formclass"></span>
                                </div>
                            </div>
                        </div>
                        <div class='col-sm-4'>
                            <label for="first-name">Due Date <span class="ClientProfileLabelColor">*</span></label>
                            <div class="form-group">
                                <div class='input-group'>
                                    <input type="text" name="duedate" id="txt_due_date" class="form-control col-md-12 col-sm-12 col-xs-12 sa-textbox sa-date" />
                                    <span class="input-group-addon formclass"></span>
                                </div>
                            </div>
                        </div>
                    </div>
