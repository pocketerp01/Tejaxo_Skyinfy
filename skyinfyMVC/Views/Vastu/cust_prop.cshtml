    function fileselected(ctrl) {

        var files = ctrl.files;
        var cnts = files.length;
        $(ctrl).parent().parent().find("span").css("color", "blue");
        $(ctrl).parent().parent().find("span")[0].innerText = "";
        for (var k = 0; k < cnts; k++) {

            var file = files[k];
            var namelen = file.name.length;
            if (namelen > 40) {
                $(ctrl).parent().parent().find("span")[0].innerText = "Sorry!! The file name can not be more than 35 Chars";
                $(ctrl).parent().parent().find("span").css("color", "red");
                return;
            }
            var ext = file.name.match(/\.(.+)$/)[1];
            switch (ext) {
                case 'pdf':
                case 'jpg':
                case 'jpeg':
                case 'png':
                case 'bmp':
                    break
                default:
                    $(ctrl).parent().parent().find("span")[0].innerText = "Sorry!! Upload only pdf,,jpg,jpeg,png,gif file";
                    $(ctrl).parent().parent().find("span").css("color", "red");
                    break;
                    break;
            }

            //var filesize = file.size / 1024 / 1024;
            var filesize = file.size / 1024 / 1024;
            if (filesize < 2) {
                var filenme = file.name.split('\\');
                var lenn = filenme.length;
                var namee = filenme[lenn - 1];

                if (cnts > 1) {

                    $(ctrl).parent().parent().find("span")[0].innerText += ", " + namee;
                    $(ctrl).parent().parent().find("span").css("color", "green");
                }
                else {
                    $(ctrl).parent().parent().find("span")[0].innerText = namee;
                    $(ctrl).parent().parent().find("span").css("color", "green");
                }
            }
            else {
                $(ctrl).parent().parent().find("span")[0].innerText = 'Sorry!! This file size is:' + filesize + 'MB Max. Allowed size is 2 MB';
                $(ctrl).parent().parent().find("span").css("color", "red");
                break;
            }
        }
    }
