    function CompareDate() {
        var today = new Date();
        var dd = today.getDate();
        var mm = today.getMonth() + 1;
        var yyyy = today.getFullYear();
        if (dd < 10) {
            dd = '0' + dd;
        }
        if (mm < 10) {
            mm = '0' + mm;
        }
        today = dd + '/' + mm + '/' + yyyy;
        var str2 = document.getElementById("txt_invdate").value;
        var dateOne = new Date(+today.split("/")[2], today.split("/")[1] - 1, +today.split("/")[0]);
        var dateTwo = new Date(+str2.split("/")[2], str2.split("/")[1] - 1, +str2.split("/")[0]);
        if (dateTwo < dateOne) {
            document.getElementById("txt_invdate").style.color = "Red";
        }
        else {
            document.getElementById("txt_invdate").style.color = "Black";
        }
    }
