            function btnokclick(selectedindex, evt) {
                debugger;
                evt.preventDefault();

                var $form = $("#fee_filter");

                $.ajax({
                    type: "POST",
                    url: "/Education/filter2",
                    data: $("form[id='fee_filter'] input,select").not("#ddmenus,#ddquick,#ctl00_HtmlEditorExtender2FontName,#ctl00_HtmlEditorExtender2FontSize").serialize() + "&command=Search",
                    dataType: "json",
                    traditional: true,
                    success: function (response) {
                        close_pop();
                    },
                    error: function (err) {
                        close_pop();
                    }
                });
            }

            function HEAD(evt) {

                evt.preventDefault();

                var $form = $("#fee_filter");


