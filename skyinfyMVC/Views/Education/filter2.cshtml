                $("[id*=divdt]").show();
                $("[id*=categoryfilter]").show();
                $("[id*=feetypefilter]").show();
                $("[id*=feeheadfilter]").show();
                if (hf_btnid == "Fee Collection")
                {
                    $("[id*=feetypefilter]").hide();
                    $("[id*=feeheadfilter]").hide();
                }
                if ((hf_btnid == "Student Due Fee") || (hf_btnid == "Family Due Fee")) {
                  
                    $("[id*=divdt]").hide();
                    $("[id*=categoryfilter]").hide();
                    $("[id*=feetypefilter]").hide();
                    $("[id*=feeheadfilter]").hide();
                }

                if ((hf_btnid == "Student Fee Receipt(With Con.)") || (hf_btnid == "Student Fee Receipt(WithOut Con.)") || (hf_btnid == "Head Wise Receipt (With Con.)") || (hf_btnid == "Head Wise Receipt (WithOut Con.)")) {
