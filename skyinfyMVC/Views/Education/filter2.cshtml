                    type: "POST",
                    url: "/Education/filter2",
                    data: $("form[id='fee_filter'] input,select").not("#ddmenus,#ddquick,#ctl00_HtmlEditorExtender2FontName,#ctl00_HtmlEditorExtender2FontSize").serialize() + "&command=Head",
