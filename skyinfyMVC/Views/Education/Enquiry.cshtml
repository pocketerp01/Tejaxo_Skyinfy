    function hidep(ctrl) {
      //  debugger
        var actrl = $(ctrl).find("a[id*=ahide]")[0];
        hide(actrl);

    }
    function hide(ctrl) {
      //  debugger
        //$(ctrl).closest("div.x_content").hide();

        if (ctrl.children[0].classList.contains('fa-chevron-up')) {
            $(ctrl.children[0]).removeClass('fa-chevron-up');
            $(ctrl.children[0]).addClass('fa-chevron-down');
            ctrl.closest("div").parentElement.children[1].style.display = 'none';
            if (ctrl.id == "ahide2") { $("#chk_sib")[0].checked = false; }
            if (ctrl.id == "ahide3") { $("#chk_familyid")[0].checked = false; }
            if (ctrl.id == "ahide8") { $("#transfer")[0].checked = false; }
            if (ctrl.id == "ahide7") { $("#chk_disability")[0].checked = false; }
        }
        else {
            $(ctrl.children[0]).removeClass('fa-chevron-down');
            $(ctrl.children[0]).addClass('fa-chevron-up');
            ctrl.closest("div").parentElement.children[1].style.display = 'block';
            if (ctrl.id == "ahide2") { $("#chk_sib")[0].checked = true; }
            if (ctrl.id == "ahide3") { $("#chk_familyid")[0].checked = true; }
            if (ctrl.id == "ahide8") { $("#chk_disability")[0].checked = true; }
        }

    }
    function hide1(ctrl, check) {

        if (check) {
            $(ctrl.children[0]).removeClass('fa-chevron-down');
            $(ctrl.children[0]).addClass('fa-chevron-up');
            ctrl.closest("div").parentElement.children[1].style.display = 'block';

        }
        else {

            $(ctrl.children[0]).removeClass('fa-chevron-up');
            $(ctrl.children[0]).addClass('fa-chevron-down');
            ctrl.closest("div").parentElement.children[1].style.display = 'none';
        }
    }

