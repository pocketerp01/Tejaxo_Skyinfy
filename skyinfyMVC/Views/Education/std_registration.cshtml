    function acnval(ctrl) {
        var s = $(ctrl).val().replace(/-/g, "");
        var ml = 1;
        var res = "";
        for (var i = 0; i < s.length; i++) {
            if (ml < 4) {
                res = res + s.charAt(i);
                ml++;
            }
            else {
                res = res + s.charAt(i) + "-";
                ml = 1;
            }
        }

        if (res.substr(res.length - 1) == "-") {
            res = res.slice(0, -1);
        }
        $(ctrl).val(res);
    }

