            }

            #snav li a {
                text-decoration: none;
                color: #fff;
                display: block;
                position: relative;
            }

            #snav .fa {
                vertical-align: middle;
                font-size: 18px;
                width: 35px;
                height: 35px;
                line-height: 35px;
                text-align: center;
                position: relative;
                z-index: 4;
