        function showmsgJS(msgtype, msg, alert_type) {
            try {
                //var lblmsg = $("[id=msgp]");
                document.getElementById("msgp").innerHTML = msg;
                //lblmsg.innerHTML = msg;
                if (msgtype == 1) {
                    $("[id=btnyes]").hide();
                    $("[id=btnno]").hide();

                    switch (alert_type) {
                        case 0:
                            $("[id=dtitle]").style.backgroundColor = "#E74C3C";
                            document.getElementById("title_type").innerHTML = "Error";
                            break;
                        case 1:
                            document.getElementById("dtitle").style.backgroundColor = "#1ABB9C";
                            document.getElementById("title_type").innerHTML = "Success";
                            break;
                        case 2:
                            document.getElementById("dtitle").style.backgroundColor = "#f0ad4e";
                            document.getElementById("title_type").innerHTML = "Warning";
                            break;
                    }

                }

                else if (msgtype == 2) {
                    $("[id=btnok]").hide();

                    document.getElementById("title_type").innerHTML = "Confirmation";
                    document.getElementById("dtitle").style.backgroundColor = "#f0ad4e";
                } else if (msgtype == 3) {
                    $("[id=btnyes]").hide();
                    $("[id=btnno]").hide();
                    $("[id=btnok]").hide();

                    switch (alert_type) {
                        case 0:
                            $("[id=dtitle]").style.backgroundColor = "#E74C3C";
                            document.getElementById("title_type").innerHTML = "Error";
                            break;
                        case 1:
                            document.getElementById("dtitle").style.backgroundColor = "#1ABB9C";
                            document.getElementById("title_type").innerHTML = "Success";
                            break;
                        case 2:
                            document.getElementById("dtitle").style.backgroundColor = "#f0ad4e";
                            document.getElementById("title_type").innerHTML = "Warning";
                            break;
                    }

                }


                showerror();
            }

            catch (eerre) {
                alert(msg);

            }



        }
