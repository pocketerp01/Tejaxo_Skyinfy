    //------------------------
    function calcme(ctrl) {
        var payment = false;
        var typee = $("#hf_type")[0].value;
        if (typee == "20" || typee == "21") {
            payment = true;
        }
        var boxes = $(ctrl);
        var billamt;
        //var billamt = $(ctrl).parent().parent().find("[id^=billamt]").val();
        var chqamt = 0;
        if ($("#chqamt")[0] != undefined) {
            chqamt = $("#chqamt")[0].value;
        }
        if (isNaN(parseFloat(chqamt))) chqamt = 0;
        var txtdrs = $("[id^=dramt]");
        var txtcrs = $("[id^=cramt]");
        var ddlmethod = $("[id^=method]");
        var totdr = 0; var totcr = 0;
        for (var k = 0; k < ddlmethod.length; k++) {
            var mth = $(ddlmethod).eq(k).val();
            if (mth == "AGR") {
                var txtdramt = $(txtdrs).eq(k);
                var txtcramt = $(txtcrs).eq(k);
                $(txtdramt).val(0);
                $(txtcramt).val(0);
                if (payment) boxes = $(txtdrs).eq(k).parent().parent().next().find("[id^=adj_amount]");
                else boxes = $(txtcrs).eq(k).parent().parent().next().find("[id^=adj_amount]");
                billamt = $(txtcrs).eq(k).parent().parent().next().find("[id^=balamt]");
                for (var b = 0; b < boxes.length; b++) {
                    var bills = $(billamt).eq(b).val();
                    if (isNaN(parseFloat(bills))) { bills = 0; }
                    bills = Math.abs(bills);
                    var myval = $(boxes).eq(b).val();
                    if (isNaN(parseFloat(myval))) { myval = 0; }
                    if (parseFloat(bills) >= parseFloat(myval)) {

                        var t = parseFloat($(txtcramt).val());
                        if (isNaN(t)) {
                            $(txtcramt).val(0); //parent row bhar di
                        }
                        var t = parseFloat($(txtdramt).val());
                        if (isNaN(t)) {
                            $(txtdramt).val(0); //parent row bhar di
                        }
                                var bills = $(billamt).eq(b).val();
                                if (bills.startsWith('-')) {
                                    $(txtdramt).val(parseFloat($(txtdramt).val()) + parseFloat(myval)); //parent row bhar di
                                }
                                else {
                                    $(txtcramt).val(parseFloat($(txtcramt).val()) + parseFloat(myval)); //parent row bhar di
                                }
                    }
                    else {
                        $(boxes).eq(b).val(0);
                        showmsgJS(1, "Adjust Amount Should Be Equal or Less Than Bill Amount, Please Check", 2);
                        return;
                    }
                }
                var dr = 0; var cr = 0;
                if (!isNaN($(txtdrs).eq(k).val())) {
                    dr = parseFloat($(txtdrs).eq(k).val());
                    totdr += dr;
                }
                if (!isNaN($(txtcrs).eq(k).val())) {
                    cr = parseFloat($(txtcrs).eq(k).val());
                    totcr += cr;
                }
            }
            else {
                var txtdrs = $("[id^=dramt]");
                var txtcrs = $("[id^=cramt]");
                var ival = 0;
                var tval = $(ctrl).val();
                if (!isNaN(parseFloat(tval))) {
                    ival = parseFloat(tval);
                }
                if (ival > 0) {
                    if ($(ctrl)[0].id.toString().trim().startsWith("dramt")) {
                        $(ctrl).parent().parent().find("[id^=cramt]").val(0);
                    } else {
                        $(ctrl).parent().parent().find("[id^=dramt]").val(0);
                    }
                }
                var dr = 0; var cr = 0;
                if (!isNaN(parseFloat($(txtdrs).eq(k).val()))) {
                    dr = parseFloat($(txtdrs).eq(k).val());
                }
                if (!isNaN(parseFloat($(txtcrs).eq(k).val()))) {
                    cr = parseFloat($(txtcrs).eq(k).val());
                }
                totdr += dr;
                totcr += cr;
            }
        }
        if (payment)
        {
            //$("#onacc").val(parseFloat(chqamt) - parseFloat(totdr));
            totcr += parseFloat(chqamt);
        }
        else
        {
            //$("#onacc").val(parseFloat(chqamt) - parseFloat(totcr));
            totdr += parseFloat(chqamt);
        }
        $("#div_dramt")[0].innerText = parseFloat(totdr);
        $("#div_cramt")[0].innerText = parseFloat(totcr);

    }
