    function checkddl() {
        try {
            var ddls = $("[id^=method]");
            var drs = $("[id^=dramt]");
            var crs = $("[id^=cramt]");
            var cnt = ddls.length;
            for (var i = 0; i < cnt; i++) {
                if (ddls[i].value == "AGR") {
                    $(crs).eq(i).attr('readonly', true);
                    $(drs).eq(i).attr('readonly', true);
                }
            }
        }
        catch (err) { alert(err); return; }
    }
    function checkdrcr() {
        try {
            var drs = $("[id^=div_dramt]").val();
            var crs = $("[id^=div_cramt]").val();
            if (drs === crs) { }
            else {
                showmsgJS(1, "Credit Amount Should Be Equal To Debit Amount, Please Check !!", 2);
