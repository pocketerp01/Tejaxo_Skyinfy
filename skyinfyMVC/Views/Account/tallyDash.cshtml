    @Html.HiddenFor(model => model[0].Col20, new { @id = "hf_dash5" })
    @Html.HiddenFor(model => model[0].Col21, new { @id = "hf_dash6" })

    @Html.HiddenFor(model => model[0].Col22, new { @id = "hf_type1" })
    @Html.HiddenFor(model => model[0].Col23, new { @id = "hf_type2" })
    @Html.HiddenFor(model => model[0].Col24, new { @id = "hf_type3" })
    @Html.HiddenFor(model => model[0].Col25, new { @id = "hf_type4" })
    @Html.HiddenFor(model => model[0].Col26, new { @id = "hf_type5" })
    @Html.HiddenFor(model => model[0].Col27, new { @id = "hf_type6" })

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_content">
                    <div class="row">
                        <div class="col-md-3 col-xs-12 widget widget_tally_box">
                            <a href="/Account/salesBook?mid=@EncryptDecrypt.Encrypt("45001.1")&m_id=@EncryptDecrypt.Encrypt(MyGuid)&tid=@EncryptDecrypt.Encrypt(Model[0].Col26)&frm_id='TACC'">
                                <div class="x_panel ui-ribbon-container fixed_height_390">
                                    <div class="x_title">
                                        <h2>Sales Book</h2>
                                        <div class="clearfix"></div>
                                    </div>
                                    <div class="x_content">
                                        <h3 class="name_title">Total Amount: </h3>
                                        <p>@Model[0].Col16</p>
                                    </div>
                                </div>
                            </a>
                        </div>
                        <div class="col-md-3 col-xs-12 widget widget_tally_box">
                            <a href="/Account/purBook?mid=@EncryptDecrypt.Encrypt("45001.2")&m_id=@EncryptDecrypt.Encrypt(MyGuid)&tid=@EncryptDecrypt.Encrypt(Model[0].Col27)&frm_id='TACC'">
