                                    <div class="x_title">
                                        <h2>Debtors</h2>
                                        <div class="clearfix"></div>
                                    </div>
                                    <div class="x_content">
                                        <h3 class="name_title">Total Amount: </h3>
                                        <p>@Model[0].Col18</p>
                                    </div>
                                </div>
                            </a>
                        </div>
                        <div class="col-md-3 col-xs-12 widget widget_tally_box">
                            <a href="/Account/creditors?mid=@EncryptDecrypt.Encrypt("45001.4")&m_id=@EncryptDecrypt.Encrypt(MyGuid)&tid=@EncryptDecrypt.Encrypt(Model[0].Col23)&frm_id='TACC'">
                                <div class="x_panel ui-ribbon-container fixed_height_390">
                                    <div class="x_title">
                                        <h2>
                                            Creditors
                                        </h2>
                                        <div class="clearfix"></div>
                                    </div>
                                    <div class="x_content">
                                        <h3 class="name_title">Total Amount: </h3>
                                        <p>@Model[0].Col19</p>
                                    </div>
                                </div>
                            </a>
                        </div>
                        <div class="col-md-3 col-xs-12 widget widget_tally_box">
                            <a href="/Account/bankBal?mid=@EncryptDecrypt.Encrypt("45001.5")&m_id=@EncryptDecrypt.Encrypt(MyGuid)&tid=@EncryptDecrypt.Encrypt(Model[0].Col24)&frm_id='TACC'">
